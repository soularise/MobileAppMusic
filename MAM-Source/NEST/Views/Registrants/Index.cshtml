@model IEnumerable<NEST.Models.Registrant>

@{
    ViewBag.Title = "Registrant";
}

<h2>Website Registrants</h2>
<p>Click "View" on a registrant below to see full details.</p>
<table>
    <tr>
        <th>
            FirstName
        </th>
        <th>
            LastName
        </th>
        <th>
            Email
        </th>
        <th>
            City
        </th>
        <th>
            State
        </th>
        <th>
            ZipCode
        </th>
        <th>Website</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZipCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WebsiteURL)
        </td>
        <td>
           <!-- @Html.ActionLink("Edit", "Edit", new { id=item.Id }) | -->
            @Html.ActionLink("View", "Edit", new { id = item.Id })
            <!--@Html.ActionLink("Delete", "Delete", new { id=item.Id }) -->
        </td>
    </tr>
}

</table>
